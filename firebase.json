{
    "functions": {
        "predeploy": "npm --prefix \"$RESOURCE_DIR\" run build",
        "source": "functions"
    },
    "extensions": {
        "send-message": "twilio/send-message@0.2.1"
    },
    "emulators": {
        "auth": {
            "port": 9099
        },
        "functions": {
            "port": 5001
        },
        "firestore": {
            "port": 8080
        },
        "pubsub": {
            "port": 8085
        },
        "ui": {
            "enabled": true
        }
    }
}
